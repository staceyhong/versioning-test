{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "group": "group-legacy",
        "src": "legacy"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "group": "group-active",
        "src": "active"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "group-legacy",
        "src": "legacy"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "group-active",
        "src": "active"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "titleSuffix": "Contoso", 
      "uhfHeaderId": "MSDocsHeader-DotNet", 
      "extendBreadcrumb": true 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "contoso",
    "groups": {
      "group-legacy": {
        "dest": "group-legacy-dest",
        "toc_rel": "./toc-different.yml",
        "moniker_range": "< contososerver-2.0"
      },
      "group-active": {
        "dest": "group-active-dest",
        "moniker_range": ">= contososerver-2.0 || >= contosoclient-1.0"
      }
    }
  }
}